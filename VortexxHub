-- StarterGui.ScreenGui.Frame.TextButton.LocalScript
local function C_5()
local script = G2L["5"];
	local button = script.Parent  -- Reference to the button
	local gui = game.StarterGui.ScreenGui.Bitt -- Reference to the GUI
	
	if gui then
		button.MouseButton1Click:Connect(function()
			gui.Visible = not gui.Visible  -- Toggle visibility when clicked
		end)
	else
		warn("MainGui not found! Make sure it's inside ScreenGui.")
	end
	
end;
task.spawn(C_5);
-- StarterGui.ScreenGui.Frame.TextButton.LocalScript
local function C_9()
local script = G2L["9"];
	local button = script.Parent  -- The activation button
	local mainFrame = script.Parent.Parent  -- The main GUI (Frame with TextBox & Button)
	local textBox = mainFrame:FindFirstChild("TextBox")  -- The input field
	local secretGui = mainFrame.Parent:FindFirstChild("MainScript")  -- The hidden GUI
	
	local correctKey = "YouAreGayXD"  -- Change this to your desired key
	
	if textBox and secretGui and mainFrame then
		button.MouseButton1Click:Connect(function()
			if textBox.Text == correctKey then
				mainFrame.Visible = false  -- Hide the main GUI
				secretGui.Visible = true   -- Show the secret GUI
			else
				textBox.Text = "Wrong Key!"  -- Feedback message
				wait(1)
				textBox.Text = ""  -- Clear input after 1 second
			end
		end)
	else
		warn("UI elements not found! Check your GUI names.")
	end
	
end;
task.spawn(C_9);
-- StarterGui.ScreenGui.Frame.LocalScript
local function C_f()
local script = G2L["f"];
	local gui = script.Parent
	local dragging = false
	local dragInput, mousePos, guiPos
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			mousePos = input.Position
			guiPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			local delta = input.Position - mousePos
			gui.Position = UDim2.new(
				guiPos.X.Scale, guiPos.X.Offset + delta.X,
				guiPos.Y.Scale, guiPos.Y.Offset + delta.Y
			)
		end
	end)
	
end;
task.spawn(C_f);
-- StarterGui.ScreenGui.MainScript.Frame.TextButton.LocalScript
local function C_1b()
local script = G2L["1b"];
	local button = script.Parent
	local replicatedStorage = game:GetService("ReplicatedStorage")
	local changeSkyEvent = replicatedStorage:FindFirstChild("ChangeSkyEvent")
	
	if changeSkyEvent then
		button.MouseButton1Click:Connect(function()
			changeSkyEvent:FireServer()  -- Tell the server to change the sky
		end)
	else
		warn("ChangeSkyEvent not found in ReplicatedStorage!")
	end
	
end;
task.spawn(C_1b);
-- StarterGui.ScreenGui.MainScript.Frame.TextButton.LocalScript
local function C_1e()
local script = G2L["1e"];
	local button = script.Parent
	local replicatedStorage = game:GetService("ReplicatedStorage")
	local destroyEvent = replicatedStorage:FindFirstChild("DestroyBaseplateEvent")
	
	if destroyEvent then
		button.MouseButton1Click:Connect(function()
			destroyEvent:FireServer() -- Request the server to destroy the Baseplate
		end)
	else
		warn("DestroyBaseplateEvent not found in ReplicatedStorage!")
	end
	
end;
task.spawn(C_1e);
-- StarterGui.ScreenGui.MainScript.Frame.TextButton.LocalScript
local function C_21()
local script = G2L["21"];
	local button = script.Parent
	local sound = game:GetService("StarterGui"):FindFirstChild("SpookyMusic") or game:GetService("Workspace"):FindFirstChild("SpookyMusic")
	local sound1=Instance.new("Sound")
	sound1.Name="SpookyMusic"
	sound1.Volume=2
	sound1.Looped=true
	sound1.SoundId="rbxassetid://71019930863855"
	sound1.Parent=game:GetService("StarterGui")
	
	button.MouseButton1Click:Connect(function()
		sound1:Play()
	end)
	
end;
task.spawn(C_21);
-- StarterGui.ScreenGui.MainScript.Frame.TextButton.LocalScript
local function C_24()
local script = G2L["24"];
	local button = script.Parent
	local lighting = game:GetService("Lighting")
	local sound = game.Workspace:FindFirstChild("DiscoMusic")
	sound2=Instance.new("Sound")
	sound2.Parent=game.Workspace
	sound2.SoundId="rbxassetid://1"
	sound2.Volume=0.5
	
	local isDiscoActive = false
	
	-- Function to change lighting colors
	local function startDisco()
		isDiscoActive = true
		sound2:Play()
	
		while isDiscoActive do
			lighting.Ambient = Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
			lighting.OutdoorAmbient = Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
			lighting.ColorShift_Top = Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
			lighting.ColorShift_Bottom = Color3.fromRGB(math.random(0, 255), math.random(0, 255), math.random(0, 255))
	
			button.Text = "Disco"
			wait(0.3) -- Flash speed
		end
	end
	
	-- Function to stop the disco
	local function stopDisco()
		isDiscoActive = false
		sound2:Stop()
		lighting.Ambient = Color3.fromRGB(127, 127, 127)
		lighting.OutdoorAmbient = Color3.fromRGB(127, 127, 127)
		lighting.ColorShift_Top = Color3.fromRGB(255, 255, 255)
		lighting.ColorShift_Bottom = Color3.fromRGB(255, 255, 255)
		button.Text = "Disco"
	end
	
	-- Button Click Event
	button.MouseButton1Click:Connect(function()
		if isDiscoActive then
			stopDisco()
		else
			startDisco()
		end
	end)
	
end;
task.spawn(C_24);
